{
  "expo": {
    "name": "DM",
    "slug": "daily-money-app",
    "icon": "./assets/DailyMoney.png",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "dailymoneyapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.sathish2882sk.dailymoneyapp",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/DailyMoney.png",
        "backgroundColor": "#E6F4FE"
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/DailyMoney.png"
    },
    "entryPoint": "expo-router/entry",
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/DailyMoney.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-router",
      "@react-native-firebase/app"
    ],
    "sdkVersion": "54.0.0",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d9ac4d8a-920c-4a0f-b9fd-485d5a7e4d9b"
      }
    }
  }
}
